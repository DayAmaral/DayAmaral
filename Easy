
public class Tabuada7 {

	public static void main(String[] args) {
		// TABUADA DO 7 (1 a 10)
		
		int numeroTabuada=7;
		
		System.out.println("Tabuada do"+ numeroTabuada +":");
		
		for(int i=1; i <= 10 ; i++) {
			
			int resultado = numeroTabuada * i;
		
		System.out.println(numeroTabuada + "X" + i + "=" + resultado);
		
		}

	}
 
}
